<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>NAV_TM_createAssigneeIncludeFilter</name>
        <uuid>_a-0000db61-5af0-8000-4594-010000010000_31271</uuid>
        <description>Generates an 'includes' filter on taskAssignees using the ri!assignee text parameter. Primarily used within an applyComponent().</description>
        <parentUuid>_a-0000db6c-eae6-8000-927a-010000010000_48231</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;SYSTEM_SYSRULES_queryFilter&quot;(
  field: &quot;taskAssignees&quot;,
  operator: &quot;includes&quot;,
  value: stripwith(ri!assignee,&quot;[]&quot;)
)</definition>
        <namedTypedValue>
            <name>assignee</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
