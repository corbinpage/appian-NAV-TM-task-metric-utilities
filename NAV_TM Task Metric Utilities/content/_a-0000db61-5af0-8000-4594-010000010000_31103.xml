<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>NAV_TM_returnDistinctTaskMetricIndices</name>
        <uuid>_a-0000db61-5af0-8000-4594-010000010000_31103</uuid>
        <description>Given a list of task metrics, this rule returns the indices of the first appearance of each task metric and ignores duplicates.</description>
        <parentUuid>_a-0000db6c-eae6-8000-927a-010000010000_48230</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(
local!uniqueTaskIds:union(ri!taskMetrics.taskId,ri!taskMetrics.taskId),
apply(#&quot;_a-0000db61-5af0-8000-4594-010000010000_31104&quot;,local!uniqueTaskIds,ri!taskMetrics.taskId)
)</definition>
        <namedTypedValue>
            <name>taskMetrics</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
